{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "max-len": ["error", { "code": 120 }],
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "semi": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off",
        "operator-linebreak": "off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}
