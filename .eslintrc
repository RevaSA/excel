{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "semi": "off",
        "arrow-parens": "off",
        "require-jsdoc": "off"
    },
    "extends": ["eslint:recommended", "google"]
}
